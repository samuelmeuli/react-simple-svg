{
	"name": "react-simple-svg",
	"version": "1.0.1",
	"description": "Inline SVGs in React made easy",
	"keywords": [
		"svg",
		"inline",
		"inline-svg",
		"vector-graphics",
		"react",
		"component",
		"react-component"
	],
	"main": "lib/SimpleSvg.js",
	"author": "Samuel Meuli <me@samuelmeuli.com>",
	"repository": "github:samuelmeuli/react-simple-svg",
	"homepage": "https://samuelmeuli.github.io/react-simple-svg",
	"license": "MIT",
	"scripts": {
		"start": "npm-run-all build:rollup --parallel start:*",
		"start:rollup": "rollup --config --sourcemap --watch",
		"start:storybook": "start-storybook --port 3000 -s stories/public",
		"build": "run-s build:*",
		"build:rollup": "rollup --config",
		"build:storybook": "build-storybook --output-dir dist -s stories/public",
		"lint": "eslint ."
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint",
			"pre-push": "yarn lint"
		}
	},
	"dependencies": {
		"prop-types": "^15.6.2"
	},
	"peerDependencies": {
		"react": "^16.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@storybook/addon-storysource": "^3.4.10",
		"@storybook/addons": "^3.3.0",
		"@storybook/react": "^3.4.10",
		"babel-core": "npm:@babel/core",
		"eslint": "^5.5.0",
		"eslint-config-airbnb": "17.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.11.1",
		"husky": "^1.0.0-rc.13",
		"npm-run-all": "^4.1.3",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"rollup": "^0.65.0",
		"rollup-plugin-babel": "^4.0.2",
		"svg-inline-loader": "^0.8.0"
	},
	"files": [
		"lib/"
	]
}
